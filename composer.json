{
    "name": "iuvabo/cakephp3-soft-delete-3.8",
    "description": "SoftDelete plugin for CakePHP 3.8",
    "keywords": [
        "cakephp",
        "cakephp 3",
        "plugin",
        "soft",
        "delete",
        "deletable"
    ],
    "homepage": "https://github.com/iuvabo/cakephp3-soft-delete-3.8",
    "type": "cakephp-plugin",
    "license": "MIT",
    "support": {
        "source": "https://github.com/iuvabo/cakephp3-soft-delete-3.8"
    },
    "require": {
        "php": ">=5.4",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "SoftDelete\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoftDelete\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}